  "model": "sonnet",
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "osascript -e 'display notification \"claude code\" with title \"Claude Code\" subtitle \"入力待ちです\"'"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/hisakazu/.claude/hooks/check-e2e-target-api-uri.sh"
          },
          {
            "type": "command",
            "command": "/Users/hisakazu/.claude/hooks/check-cd-and-command.sh"
          },
          {
            "type": "command",
            "command": "/Users/hisakazu/.claude/hooks/check-rm-command.sh"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/hisakazu/.claude/hooks/check-plan-build-commands.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/hisakazu/.claude/hooks/check-completion.sh"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "/Users/hisakazu/.claude/statusline-command.sh"
  },
  "enabledPlugins": {
    "gopls-lsp@claude-plugins-official": true,
    "agent-sdk-dev@claude-plugins-official": true,
    "plugin-dev@claude-plugins-official": true,
    "greptile@claude-plugins-official": true
  },
  "language": "日本語",
  "alwaysThinkingEnabled": true,
  "autoUpdatesChannel": "stable"
}
